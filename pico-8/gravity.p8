--gravman
--brennan rodriguez
objects = {} -- the objects in the world
cam = {} -- variables to track the camera
cam.x_offset = 0
cam.y_offset = 201

mode = 0--0 is menu, 1 is game, 2 is victory, 3 is crash
music(0)
frame = 0

function make_object(name,sprite,x,y)

obj = {}
obj.name = name
obj.spr = sprite
obj.x = x
obj.y = y
obj.gravity = 0
obj.speed = 0
add(objects,obj)

return obj
end

function _init()
local playerobj = make_object("player",1,cam.x_offset,cam.y_offset + 64)
playerobj.gravity = 2
playerobj.speed = 2

end
--resets the game
function reset()
cam.x_offset = 0
cam.y_offset = 201
tp = getplayer()
tp.x = cam.x_offset
tp.y = cam.y_offset + 64
updateplayer(tp)
end

function _update()
frame += 1
if(frame == 256) music(1)
if(frame == 512) music(2)
if(frame == 768) music(3)

if(frame == 1024) then
music(4)
frame = 0
end

--player logic
if(mode == 0) then

	cam.x_offset = 0
	cam.y_offset = 0
	if(btnp(2))then
		mode = 1
		reset()
	end
end
		tp = getplayer()
	if(mode == 1) then

	if(btnp(4) or btnp(5)) then
		tp.gravity *= -1
	end
	if(btnp(0)) then
		tp.x -= 1
	end
	if(btnp(1)) then
		tp.x += 1
	end
	tp.spr = 1
	if(will_player_collide_vertically() == false) then
		moveplayer(0, getplayer().gravity)
		cam.y_offset += getplayer().gravity
		tp.spr = 2
	end
	if(is_player_crashed() == false) then
		moveplayer(getplayer().speed, 0)
		cam.x_offset += getplayer().speed
	else
		mode = 3
	end
	if(is_player_won()) then
		mode = 2
 	end

	end
if(mode == 2)then
	cam.x_offset = 0
	cam.y_offset = 0

end
if(mode == 3) then
	cam.x_offset = 0
	cam.y_offset = 0
	if(btnp(4) or btnp(5)) then
		reset()
		mode = 1
	end
end
		camera(cam.x_offset,cam.y_offset)
end


function render_object(obj)
spr(obj.spr,obj.x,obj.y)

end


function _draw()
cls()
if(mode == 0) then
map(0,0,0,0,127,63)
print("gravity - brennan rodriguez", 20,0,12)
print("press x or z to change gravity", 0, 64, 12)
print("press up to start", 0, 80, 12)
end
if(mode == 1)then
circfill(20,20,10,1)
foreach(objects, render_object)
map(0,0,0,0,127,63)
end

if(mode == 2) then
print("you did it!!",0,0,7)
print("thanks for playing♥",0,16,12)

end
if(mode == 3) then
print("you died 😐.",0,0,7)
print("press z or x to retry",0,8,7)

end
--print(frame, 64, 120, 7)
end



